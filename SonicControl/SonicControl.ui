<?xml version='1.0' encoding='utf-8'?>
<interface version="1.0">
  <object class="ttk.Frame" id="SonicControl">
    <property name="borderwidth">0</property>
    <property name="height">680</property>
    <property name="width">520</property>
    <layout manager="pack">
      <property name="propagate">True</property>
      <property name="side">top</property>
    </layout>
    <child>
      <object class="ttk.Notebook" id="notebook_1">
        <property name="height">680</property>
        <property name="width">530</property>
        <layout manager="pack">
          <property name="expand">true</property>
          <property name="fill">both</property>
          <property name="propagate">True</property>
          <property name="side">top</property>
        </layout>
        <child>
          <object class="ttk.Notebook.Tab" id="tabManual">
            <property name="padding">0</property>
            <property name="state">normal</property>
            <property name="sticky">nsew</property>
            <property name="text" translatable="yes">Manual</property>
            <child>
              <object class="ttk.Frame" id="frame_3">
                <property name="height">200</property>
                <property name="width">200</property>
                <layout manager="pack">
                  <property name="propagate">True</property>
                  <property name="side">top</property>
                </layout>
                <child>
                  <object class="ttk.Labelframe" id="MHzFrame">
                    <property name="height">200</property>
                    <property name="text" translatable="yes">MHz Mode</property>
                    <property name="width">200</property>
                    <layout manager="pack">
                      <property name="fill">x</property>
                      <property name="padx">10</property>
                      <property name="pady">10</property>
                      <property name="propagate">True</property>
                      <property name="side">top</property>
                    </layout>
                    <child>
                      <object class="ttk.Label" id="FreqLabel">
                        <property name="text" translatable="yes">Frequency / Hz</property>
                        <layout manager="pack">
                          <property name="anchor">nw</property>
                          <property name="pady">5</property>
                          <property name="propagate">True</property>
                          <property name="side">top</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="ttk.Spinbox" id="MHzFreqSpinBox">
                        <property name="command">setMHzFrequency</property>
                        <property name="from_">900000</property>
                        <property name="increment">100</property>
                        <property name="justify">left</property>
                        <property name="text" translatable="yes">spinbox_1</property>
                        <property name="to">6000000</property>
                        <property name="validate">none</property>
                        <property name="width">30</property>
                        <layout manager="pack">
                          <property name="anchor">nw</property>
                          <property name="pady">5</property>
                          <property name="propagate">True</property>
                          <property name="side">top</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="ttk.Spinbox" id="ScrollDigitSpinbox">
                        <property name="command">setScrollDigit</property>
                        <property name="from_">1</property>
                        <property name="increment">1</property>
                        <property name="state">normal</property>
                        <property name="text" translatable="yes">3</property>
                        <property name="to">6</property>
                        <property name="validate">none</property>
                        <property name="width">3</property>
                        <layout manager="pack">
                          <property name="anchor">nw</property>
                          <property name="padx">5</property>
                          <property name="pady">5</property>
                          <property name="propagate">True</property>
                          <property name="side">right</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="ttk.Label" id="ScrollDigitLabel">
                        <property name="text" translatable="yes">Scroll digit</property>
                        <layout manager="pack">
                          <property name="anchor">nw</property>
                          <property name="padx">5</property>
                          <property name="pady">5</property>
                          <property name="propagate">True</property>
                          <property name="side">right</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="ttk.Label" id="GainLabel">
                        <property name="text" translatable="yes">Gain / %</property>
                        <layout manager="pack">
                          <property name="anchor">nw</property>
                          <property name="pady">10</property>
                          <property name="propagate">True</property>
                          <property name="side">top</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="ttk.Spinbox" id="MHzGainSpinBox">
                        <property name="command">setMHzGain</property>
                        <property name="from_">0</property>
                        <property name="increment">1</property>
                        <property name="text" translatable="yes">spinbox_2</property>
                        <property name="to">100</property>
                        <property name="width">30</property>
                        <layout manager="pack">
                          <property name="anchor">nw</property>
                          <property name="propagate">True</property>
                          <property name="side">top</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="ttk.Button" id="SetMHzFreqButton">
                        <property name="command">setMHzFrequency</property>
                        <property name="text" translatable="yes">Set Frequency and Gain</property>
                        <layout manager="pack">
                          <property name="anchor">ne</property>
                          <property name="pady">5</property>
                          <property name="propagate">True</property>
                          <property name="side">right</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="ttk.Button" id="MHzModeButton">
                        <property name="command">setMHzMode</property>
                        <property name="text" translatable="yes">Set MHz Mode</property>
                        <layout manager="pack">
                          <property name="anchor">ne</property>
                          <property name="pady">5</property>
                          <property name="propagate">True</property>
                          <property name="side">bottom</property>
                        </layout>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="ttk.Labelframe" id="kHzFrame">
                    <property name="height">200</property>
                    <property name="text" translatable="yes">kHz Mode</property>
                    <property name="width">200</property>
                    <layout manager="pack">
                      <property name="fill">x</property>
                      <property name="padx">10</property>
                      <property name="pady">10</property>
                      <property name="propagate">True</property>
                      <property name="side">top</property>
                    </layout>
                    <child>
                      <object class="ttk.Label" id="kHzFreqLabel">
                        <property name="text" translatable="yes">Frequency / Hz</property>
                        <layout manager="pack">
                          <property name="anchor">nw</property>
                          <property name="pady">5</property>
                          <property name="propagate">True</property>
                          <property name="side">top</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="ttk.Spinbox" id="kHzFreqSpinBox">
                        <property name="from_">50000</property>
                        <property name="increment">100</property>
                        <property name="state">normal</property>
                        <property name="text" translatable="yes">spinbox_3</property>
                        <property name="to">1100000</property>
                        <property name="width">30</property>
                        <layout manager="pack">
                          <property name="anchor">nw</property>
                          <property name="pady">5</property>
                          <property name="propagate">True</property>
                          <property name="side">top</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="ttk.Button" id="SetkHzFreqButton">
                        <property name="command">setkHzFrequency</property>
                        <property name="text" translatable="yes">Set kHz Frequency</property>
                        <layout manager="pack">
                          <property name="propagate">True</property>
                          <property name="side">right</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="ttk.Button" id="kHzModeButton">
                        <property name="command">setkHzMode</property>
                        <property name="text" translatable="yes">Set kHz Mode</property>
                        <layout manager="pack">
                          <property name="propagate">True</property>
                          <property name="side">right</property>
                        </layout>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="ttk.Button" id="setUSActiveButton">
                    <property name="command">setUSActive</property>
                    <property name="text" translatable="yes">US on</property>
                    <layout manager="pack">
                      <property name="pady">40</property>
                      <property name="propagate">True</property>
                      <property name="side">top</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="ttk.Button" id="SetUSInActiveButton">
                    <property name="command">SetUSInActive</property>
                    <property name="text" translatable="yes">US off</property>
                    <layout manager="pack">
                      <property name="pady">10</property>
                      <property name="propagate">True</property>
                      <property name="side">top</property>
                    </layout>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="ttk.Notebook.Tab" id="tabAutomation">
            <property name="text" translatable="yes">Scripting</property>
            <child>
              <object class="ttk.Frame" id="frame_4">
                <property name="height">200</property>
                <property name="width">200</property>
                <layout manager="pack">
                  <property name="propagate">True</property>
                  <property name="side">top</property>
                </layout>
                <child>
                  <object class="ttk.Frame" id="ButtonsAuto">
                    <property name="height">200</property>
                    <property name="width">200</property>
                    <layout manager="pack">
                      <property name="anchor">w</property>
                      <property name="fill">y</property>
                      <property name="propagate">True</property>
                      <property name="side">left</property>
                    </layout>
                    <child>
                      <object class="ttk.Button" id="LoadScriptButton">
                        <property name="command">loadFile</property>
                        <property name="text" translatable="yes">Load Script File</property>
                        <layout manager="pack">
                          <property name="pady">5</property>
                          <property name="propagate">True</property>
                          <property name="side">top</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="ttk.Button" id="SaveScriptButton">
                        <property name="command">saveFile</property>
                        <property name="text" translatable="yes">Save Script File</property>
                        <layout manager="pack">
                          <property name="pady">5</property>
                          <property name="propagate">True</property>
                          <property name="side">top</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="ttk.Button" id="SaveLogButton">
                        <property name="command">openLogFile</property>
                        <property name="text" translatable="yes">Save Log File to</property>
                        <layout manager="pack">
                          <property name="padx">5</property>
                          <property name="pady">5</property>
                          <property name="propagate">True</property>
                          <property name="side">top</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="ttk.Button" id="StopScriptButton">
                        <property name="command">closeFile</property>
                        <property name="text" translatable="yes">Stop Script</property>
                        <layout manager="pack">
                          <property name="padx">5</property>
                          <property name="pady">5</property>
                          <property name="propagate">True</property>
                          <property name="side">bottom</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="ttk.Button" id="StartScript">
                        <property name="command">readFile</property>
                        <property name="text" translatable="yes">Start Script</property>
                        <layout manager="pack">
                          <property name="padx">5</property>
                          <property name="pady">5</property>
                          <property name="propagate">True</property>
                          <property name="side">bottom</property>
                        </layout>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="ttk.Labelframe" id="ScriptFrame">
                    <property name="height">200</property>
                    <property name="text" translatable="yes">Script Editor</property>
                    <property name="width">200</property>
                    <bind add="" handler="callback" sequence="&lt;1&gt;" />
                    <layout manager="pack">
                      <property name="anchor">nw</property>
                      <property name="expand">true</property>
                      <property name="fill">both</property>
                      <property name="padx">3</property>
                      <property name="pady">5</property>
                      <property name="propagate">True</property>
                      <property name="side">top</property>
                    </layout>
                    <child>
                      <object class="tk.Text" id="text_1">
                        <property name="autoseparators">false</property>
                        <property name="font">TkDefaultFont</property>
                        <property name="height">10</property>
                        <property name="insertunfocussed">solid</property>
                        <property name="text" translatable="yes">Enter tasks here...</property>
                        <property name="width">50</property>
                        <layout manager="pack">
                          <property name="anchor">nw</property>
                          <property name="expand">true</property>
                          <property name="fill">both</property>
                          <property name="padx">3</property>
                          <property name="pady">3</property>
                          <property name="propagate">True</property>
                          <property name="side">left</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="ttk.Scrollbar" id="scrollbar">
                        <property name="orient">vertical</property>
                        <layout manager="pack">
                          <property name="anchor">n</property>
                          <property name="expand">false</property>
                          <property name="fill">y</property>
                          <property name="propagate">True</property>
                          <property name="side">left</property>
                        </layout>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="ttk.Label" id="label_1">
                    <property name="font">{Futura Md BT} 12 {}</property>
                    <property name="text" translatable="yes">Current task:</property>
                    <layout manager="pack">
                      <property name="propagate">True</property>
                      <property name="side">left</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="ttk.Label" id="TaskLabel">
                    <property name="font">{Futura Md BT} 12 {}</property>
                    <property name="text" translatable="yes">Idle</property>
                    <property name="textvariable">string:self.CurrentTask</property>
                    <layout manager="pack">
                      <property name="propagate">True</property>
                      <property name="side">top</property>
                    </layout>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="ttk.Notebook.Tab" id="TabConnection">
            <property name="text" translatable="yes">Connection</property>
            <child>
              <object class="ttk.Frame" id="frame_5">
                <property name="height">200</property>
                <property name="width">200</property>
                <layout manager="pack">
                  <property name="propagate">True</property>
                  <property name="side">top</property>
                </layout>
                <child>
                  <object class="ttk.Labelframe" id="COMPortsFrame">
                    <property name="height">200</property>
                    <property name="text" translatable="yes">SonicAmp COM Port</property>
                    <property name="width">200</property>
                    <layout manager="pack">
                      <property name="fill">x</property>
                      <property name="padx">10</property>
                      <property name="pady">10</property>
                      <property name="propagate">True</property>
                      <property name="side">top</property>
                    </layout>
                    <child>
                      <object class="ttk.Combobox" id="comboB1">
                        <property name="command">closePort</property>
                        <property name="textvariable">string:self.varPort</property>
                        <property name="values">self.listdev</property>
                        <property name="width">30</property>
                        <layout manager="pack">
                          <property name="padx">5</property>
                          <property name="propagate">True</property>
                          <property name="side">left</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="ttk.Button" id="RefreshButton">
                        <property name="command">getPorts</property>
                        <property name="text" translatable="yes">Refresh</property>
                        <layout manager="pack">
                          <property name="padx">5</property>
                          <property name="propagate">True</property>
                          <property name="side">left</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="ttk.Button" id="ConnectButton">
                        <property name="command">connectPort</property>
                        <property name="text" translatable="yes">Connect SonicAmp</property>
                        <layout manager="pack">
                          <property name="padx">5</property>
                          <property name="propagate">True</property>
                          <property name="side">top</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="ttk.Button" id="ClosePortButton">
                        <property name="command">closePorts</property>
                        <property name="text" translatable="yes">Close SonicAmp</property>
                        <layout manager="pack">
                          <property name="padx">5</property>
                          <property name="pady">5</property>
                          <property name="propagate">True</property>
                          <property name="side">top</property>
                        </layout>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="ttk.Labelframe" id="FirmwareFrame">
                    <property name="height">200</property>
                    <property name="text" translatable="yes">SonicAmp Firmware Info</property>
                    <property name="width">200</property>
                    <layout manager="pack">
                      <property name="pady">30</property>
                      <property name="propagate">False</property>
                      <property name="side">top</property>
                    </layout>
                    <child>
                      <object class="ttk.Label" id="FirmwareInfo">
                        <property name="text" translatable="yes">No SonicAmp connected</property>
                        <property name="textvariable">string:self.FirmwareInfoText</property>
                        <layout manager="pack">
                          <property name="padx">10</property>
                          <property name="pady">10</property>
                          <property name="propagate">True</property>
                          <property name="side">top</property>
                        </layout>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="ttk.Notebook.Tab" id="tabInfo">
            <property name="text" translatable="yes">Info</property>
            <child>
              <object class="ttk.Frame" id="frame_6">
                <property name="height">200</property>
                <property name="width">200</property>
                <layout manager="pack">
                  <property name="propagate">True</property>
                  <property name="side">top</property>
                </layout>
                <child>
                  <object class="ttk.Labelframe" id="DisclaimerFrame">
                    <property name="height">200</property>
                    <property name="text" translatable="yes">Disclaimer</property>
                    <property name="width">200</property>
                    <layout manager="pack">
                      <property name="anchor">center</property>
                      <property name="fill">both</property>
                      <property name="padx">20</property>
                      <property name="pady">20</property>
                      <property name="propagate">True</property>
                      <property name="side">top</property>
                    </layout>
                    <child>
                      <object class="ttk.Label" id="DisclaimerLabel">
                        <property name="borderwidth">5</property>
                        <property name="font">TkDefaultFont</property>
                        <property name="justify">left</property>
                        <property name="padding">20</property>
                        <property name="text" translatable="yes">SonicControl v0.3

A lightweight GUI for remote control of soniccatch and sonicwipe over
the serial interface.
Allows automation of processes through the scripting editor.

(c) usePAT GmbH</property>
                        <layout manager="pack">
                          <property name="anchor">center</property>
                          <property name="fill">both</property>
                          <property name="padx">10</property>
                          <property name="pady">10</property>
                          <property name="propagate">True</property>
                          <property name="side">top</property>
                        </layout>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="statusframe">
        <property name="height">25</property>
        <property name="relief">ridge</property>
        <property name="width">200</property>
        <layout manager="pack">
          <property name="fill">both</property>
          <property name="padx">1</property>
          <property name="pady">2</property>
          <property name="propagate">False</property>
          <property name="side">top</property>
        </layout>
        <child>
          <object class="ttk.Label" id="ConnectionStatus">
            <property name="background">#ff8080</property>
            <property name="text" translatable="yes">No SonicAmp connected</property>
            <layout manager="pack">
              <property name="padx">5</property>
              <property name="propagate">True</property>
              <property name="side">left</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="StatusScript">
            <property name="background">#ff8080</property>
            <property name="justify">right</property>
            <property name="text" translatable="yes">No script started</property>
            <property name="textvariable">string:scriptstatus</property>
            <layout manager="pack">
              <property name="padx">5</property>
              <property name="propagate">True</property>
              <property name="side">right</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="USStateLabel">
            <property name="background">#ff8080</property>
            <property name="justify">center</property>
            <property name="text" translatable="yes">US emission off</property>
            <layout manager="pack">
              <property name="padx">90</property>
              <property name="propagate">True</property>
              <property name="side">left</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
